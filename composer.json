{
    "name": "cms/backend-common",
    "version": "1.5.1",
    "type": "library",
    "description": "Backend common package",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": "7.2.5",
        "ext-json": "*",
        "symfony/doctrine-bridge": "4.4.12",
        "symfony/form": "4.4.12",
        "symfony/orm-pack": "1.2.0",
        "symfony/validator": "4.4.12",
        "symfony/yaml": "4.4.12",
        "zircote/swagger-php": "^3.0.1"
    },
    "require-dev": {
        "phpro/grumphp": "^0.17.2",
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Cms\\Backend\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cms\\Backend\\Common\\Tests\\": "tests/"
        }
    }
}
